    const isConnected = ref(false);
    let callTimer = null;
    let callStartTime = 0;
    const keypadKeys = [
      { value: "1", primary: "1", secondary: "" },
      { value: "2", primary: "2", secondary: "ABC" },
      { value: "3", primary: "3", secondary: "DEF" },
      { value: "4", primary: "4", secondary: "GHI" },
      { value: "5", primary: "5", secondary: "JKL" },
